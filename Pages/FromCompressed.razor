@page "/FromCompressed"

@using Models;
@using graphics_encoding.Components;

<div class="text-raster-pair">
    <DisplayRaster Raster="rasterSmiley" ShowMismatchIndicators="false"/>
</div>
<br>

@code {
    string inRasterSmiley = "1 1 1 1 1" + Environment.NewLine +
                            "1 1 1 1 1" + Environment.NewLine +
                            "5" + Environment.NewLine +
                            "0 5" + Environment.NewLine +
                            "1 3 1" + Environment.NewLine;
    Raster rasterSmiley;

    protected override void OnInitialized()
    {
        Raster rasterSmiley = new Raster(inRasterSmiley, 0, false, false, true);
    }
}
